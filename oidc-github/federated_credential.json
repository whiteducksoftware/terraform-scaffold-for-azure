{
    "name": "<stage>-github-actions",
    "issuer": "https://token.actions.githubusercontent.com",
    "claimsMatchingExpression": {
        "value": "claims['sub'] matches 'repo:<organizationName>/<repositoryName>:environment:<environmentName>'",
        "languageVersion": 1
    },
    "description": "Terraform pipeline via GitHub Actions",
    "audiences": [
        "api://AzureADTokenExchange"
    ]
}